<p style="color: green"><%= notice %></p>

<% content_for :title, "Programs" %>

<h1>Programs</h1>

<div id="programs">
  <% @programs.each do |program| %>
    <%= render "programname", program: program %>
    <p>
      <%= link_to "Show this program", program %>
    </p>
  <% end %>
</div>

<%= link_to "New program", new_program_path %>
